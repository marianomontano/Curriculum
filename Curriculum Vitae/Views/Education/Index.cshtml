@model IEnumerable<Curriculum_Vitae.Models.EducationModel>

<div class="h2">Educación</div>
<br />

@foreach (var item in Model)
{
    <div class="panel panel-default">
        <div class="panel-heading">
            <span class="h3">
                @Html.DisplayFor(model => item.Descripcion)
            </span>
        </div>
        <div class="panel-body">
            <div class="h4">
                @Html.DisplayNameFor(model => model.Institucion):
                @Html.DisplayFor(model => item.Institucion)<br />
            </div>

            @Html.DisplayNameFor(model => model.FechaInicio):
            @Html.DisplayFor(model => item.FechaInicio)<br />

            @Html.DisplayNameFor(model => model.FechaFin):
            @Html.DisplayFor(model => item.FechaFin)<br />

            @Html.DisplayNameFor(model => model.Estado):
            @Html.DisplayFor(model => item.Estado)<br />
        </div>
    </div>
}